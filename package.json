{
  "name": "ultra-utils",
  "version": "1.0.0",
  "description": "ðŸš€ A lightweight, zero-dependency utility library with 15+ essential functions for modern JavaScript development",
  "main": "src/index.js",
  "module": "src/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./src/index.cjs"
    },
    "./string": {
      "import": "./src/string.js",
      "require": "./src/string.cjs"
    },
    "./date": {
      "import": "./src/date.js",
      "require": "./src/date.cjs"
    },
    "./validate": {
      "import": "./src/validate.js",
      "require": "./src/validate.cjs"
    },
    "./number": {
      "import": "./src/number.js",
      "require": "./src/number.cjs"
    },
    "./array": {
      "import": "./src/array.js",
      "require": "./src/array.cjs"
    },
    "./object": {
      "import": "./src/object.js",
      "require": "./src/object.cjs"
    },
    "./misc": {
      "import": "./src/misc.js",
      "require": "./src/misc.cjs"
    }
  },
  "bin": {
    "ultra-utils": "./bin/cli.js"
  },
  "scripts": {
    "test": "node tests/sample.test.js",
    "build": "node scripts/build-cjs.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "utilities",
    "utils",
    "helpers",
    "javascript",
    "esm",
    "commonjs",
    "cli",
    "zero-dependencies",
    "lightweight",
    "tree-shakable"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ultra-utils.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/ultra-utils/issues"
  },
  "homepage": "https://github.com/yourusername/ultra-utils#readme",
  "files": [
    "src/",
    "bin/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  }
}
